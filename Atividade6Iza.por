programa {
  funcao inicio() {
    real Numero
    escreva("Informe o número e lhe digo se se está ou não, entre 10 e 30: ")
    leia(Numero)
    se (Numero <10){
    escreva("o número não está entre 10 e 30")
    }senao
    se (Numero >30){
      escreva("o número não está entre 10 e 30")
    }senao
    escreva("o número está entre 10 e 30")

  }
}
